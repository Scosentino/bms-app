<!DOCTYPE html>
<html class="no-js" lang="zxx">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!--=== Favicon ===-->
  <link rel="shortcut icon" href="assets/img/favicon.ico" type="image/x-icon" />

  <title>BMS</title>

  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    'admins', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'admins', 'data-turbolinks-track': 'reload' %>
</head>

<body class="preloader-active">

<!--== Preloader Area Start (Home2) ==-->
<div class="preloader">
  <div class="preloader-spinner">
    <div class="waves-block">
      <div class="waves wave-1"></div>
      <div class="waves wave-2"></div>
      <div class="waves wave-3"></div>
    </div>
  </div>
</div>
<!--== Preloader Area End ==-->


<!--== Header Area Start ==-->
<header id="header-area">


  <!-- Header Bottom Area Start -->
  <div class="header-bottom" id="fixheader">
    <div class="container">
      <div class="row">
        <!-- Logo Area Start -->
        <div class="col-lg-2">
          <a href="/" class="logo-area">
            <img src="https://i.imgur.com/GPKmsua.png" alt="Logo">
          </a>
        </div>
        <!-- Logo Area End -->

        <!-- Menu Area Start -->
        <div class="col-lg-7">
          <nav class="mainmenu pull-right">
          </nav>
        </div>
        <!-- Menu Area End -->

        <div class="col-lg-3 d-none d-lg-block">
          <div class="header-btn pull-right">
            <a href="register.html" class="seo-btn hvr-curl-bottom-right">Get Started Now</a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- Header Bottom Area End -->
</header>
<!--== Header Area End ==-->

<%= yield %>

<!-- PAge contnt end -->
<!--  -->

<!--== Footer Area Start (Home2) ==-->
<footer id="footer-area" class="home__2 overlay">
  <div class="footer-widget section-padding">
    <div class="container">
      <div class="row">
        <!-- Single Widget Start -->
        <div class="col-lg-4 col-md-6">
          <div class="single-widget-wrap">
            <div class="widget-title">
              <h5>About Company</h5>
            </div>
            <div class="widget-body">
              <img src="https://i.imgur.com/GPKmsua.png" alt="Logo">
              <p>Lorem ipsum dolor sit amet, consectetur to act the adipisicing elit, sed do eiusmod tempor sos what incididunt ut labore et denim.the to in the adipisicing elit.</p>
              <div class="footer-icons">
                <a href="#"><i class="fa fa-facebook"></i></a>
                <a href="#"><i class="fa fa-twitter"></i></a>
                <a href="#"><i class="fa fa-linkedin"></i></a>
              </div>
            </div>
          </div>
        </div>
        <!-- Single Widget End -->

        <!-- Single Widget Start -->
        <div class="col-lg-5 col-md-6">
          <div class="single-widget-wrap">
            <div class="widget-title">
              <h5>Service's Provided</h5>
            </div>
            <div class="widget-body">
              <ul class="footer-list">
                <li><a href="#">Merchant Rate Audit</a></li>
                <li><a href="#">Merchant Rate Negotiation</a></li>
                <li><a href="#">Merchant Analysis</a></li>

              </ul>
            </div>
          </div>
        </div>
        <!-- Single Widget End -->

        <!-- Single Widget Start -->
        <div class="col-lg-3 col-md-6">
          <div class="single-widget-wrap">
            <div class="widget-title">
              <h5>Get Started By uploading your most recent Merchant Statement</h5>
            </div>
            <div class="widget-body">
              <div class="footer-form">
                <form action="index.html">
                  <input type="text" placeholder="Full Name">
                  <input type="email" placeholder="Email Address">
                  <input type="url" placeholder="Website URL">
                  <button type="button" name="button">Merchant Statement <i class="fa fa-upload"></i> </button>
                  <hr>
                  <button class="quote-btn"><i class="fa fa-send"></i> Next</button>
                </form>
              </div>
            </div>
          </div>
        </div>
        <!-- Single Widget End -->
      </div>
    </div>
  </div>
  <div class="footer-bottom">
    <div class="container">
      <div class="row">
        <div class="col-lg-12 text-center">
          <div class="copyright-text">
            <p>Copyright Â© 2018 BMS. All Rights Reserved.</p>
          </div>
        </div>
      </div>
    </div>
  </div>
</footer>
<!--== Footer Area End ==-->

<!--== Scroll Top Area Start ==-->
<div class="scroll-top home__2">
  <i class="fa fa-angle-double-up"></i>
</div>
<!--== Scroll Top Area End ==-->

</body>

</html>